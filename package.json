{
  "name": "csmath",
  "version": "1.11.2",
  "description": "A simple math operation functions npm test",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist src/",
    "lint": "eslint src/",
    "test": "jest --coverage",
    "start": "node src/index.js",
    "validate": "npm-run-all --parallel lint test build",
    "synk-auth": "snyk auth",
    "snyk-test": "snyk test",
    "security": "npm run synk-auth && npm run snyk-test"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EricSekyere/npm-publish"
  },
  "bugs": {
    "url": "https://github.com/EricSekyere/npm-publish/issues"
  },
  "keywords": [
    "Math",
    "NodeJS",
    "JavaScript"
  ],
  "author": "Eric Sekyere",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^5.14.1",
    "ghooks": "^2.0.4",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.3",
    "snyk": "^1.134.2"
  },
  "dependencies": {
    "ajv": "^6.12.3"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "index.test.js"
    ],
    "testEnvironment": "node"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  },
  "peerDependencies": {
    "babel-core": "6.x | ^7.0.0-bridge.0"
  }
}
